{
  "version": 2,
  "builds": [
     {
       "src": "Frontend/vite-project/package.json",
       "use": "@vercel/node"
     },
     {
       "src": "Backend/package.json",
       "use": "@vercel/node"
     }
  ],
  "routes": [
     {
       "src": "/api/.*",
       "dest": "/Backend/index.js"
     },
     {
       "src": "/.*",
       "dest": "/Frontend/vite-project/dist/index.html"
     }
  ]
 }
 